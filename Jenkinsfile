    agent any
    stages{
        stage('sourcecode'){
            steps{
                echo "this is source code stage"
            }
            stage('Deploy'){
            steps{
                echo "this is deploy stage"
            }
       }
        stage('Docker'){
            steps{
                echo "this is docker stage"
            }
             stage('k8s'){
            steps{
                echo "this is k8s stage"
            }
    }
   
                
}
